* {
    margin: 0;
    padding: 0;

}

body {
    background-color: rgb(44, 44, 44);
}

.code {
    font-family: 'Space Mono';
    background-color: rgba(134, 134, 134, 0.116);
    color: white;
    display: inline-block;
    font-weight: 200;
   
    
    
    

}
.bold {
    font-weight: bold;
}
.code2 {
    font-family: 'Space Mono';
    background-color: rgba(134, 134, 134, 0.116);
    font-size: 0.85rem;
}
header h1 {
    
    font-size: 2rem;
    width: 70%;
    text-align: center;
    color: #b18840;
    margin: 2rem auto;
    font-family: 'Courier New', Courier, monospace;
    border-top: 2px solid #b18840;
    border-bottom: 2px solid #b18840;
    padding: 0.5rem 0;
    letter-spacing: 0.3rem;

}
h2  {
    font-size: 1.8rem;
    font-family: Verdana, Geneva, Tahoma, sans-serif;
    color: #b18840;
    margin-top: 7rem;
    margin-left: 2rem;
}

p {
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    color: #b18840;
    margin-left: 2.2rem;
    margin-top: 0.5rem;
}
table {
    border: 0.125rem solid #000000;
    width: 70%;
    margin: 2rem auto;
    color: white;
}

table thead {
    background-color: #b18840;
    color: black;
    font-size: 1.25rem;
    text-align: center;
    font-family: 'Share Tech';

}

table thead th {
    font-weight: 300;
}

.desc {
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    padding-top: 0.25rem;
   
}
.attr {
    text-align: center;
    
}
td {
    border: 0.1px solid grey;
    padding-left: 0.5rem;
}
#th1 {
    padding-left: 2rem;
    padding-right: 2rem;

}
#th2 {
    padding-left: 2rem;
    padding-right: 2rem;

}
.name {
    font-family: Verdana, Geneva, Tahoma, sans-serif;
}
.bexmp {
    text-align: center;
}
